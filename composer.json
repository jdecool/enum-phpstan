{
    "name": "jdecool/enum-phpstan",
    "type": "library",
    "description": "PHPStan extension for jdecool/enum",
    "license": "MIT",
    "authors": [
        {
            "name": "Jérémy DECOOL",
            "email": "contact@jdecool.fr"
        }
    ],
    "require": {
        "php": "^8.0",
        "jdecool/enum": "^1.0",
        "phpstan/phpstan": "^0.12"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "vimeo/psalm": "^4.3"
    },
    "autoload": {
        "psr-4": {
            "JDecool\\EnumPHPStan\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JDecool\\EnumPHPStan\\Test\\": "tests/"
        }
    }
}
